#include <Servo.h>

Servo myServo;  // Create a servo object

void setup() 
{
    myServo.attach(2);  // Attach the servo to pin D2
}

void loop()
{
    myServo.write(20);  //close position
    delay(1000);     
    myServo.write(70);  // open position
    delay(1000);        // Wait for a second
}
